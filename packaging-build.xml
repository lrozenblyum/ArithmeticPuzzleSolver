<?xml version="1.0" encoding="UTF-8"?>
<project name="Packaging Generator" default="_packaging_generation_">
<target name="_packaging_generation_" depends="CREATE"/>
	
<target name ="REMOVE" description="Remove">
	<delete file="deploy/APZ.jar" />
</target>
	
<target name="CREATE" description="APZ.jar" depends="REMOVE">
	<mkdir dir="deploy"/>
	<jar destfile="deploy/APZ.jar" basedir="classes" manifest="META-INF/manifest.mf">
		<fileset dir="classes" id="classesFileSet">
		    <include name="*.class"/>
		</fileset>	
	</jar>
</target>
</project>